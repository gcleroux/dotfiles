{
    "status_require": {
        "prefix": "status_ok",
        "body": [
            "local $1_status_ok, ${2:_} = pcall(require, \"${3:_}\")",
            "if not $1_status_ok then",
            "\tvim.notify(\"Plugin $3 is missing\")",
            "\treturn",
            "end",
            "\n"
        ],
        "description": "Create the status_ok call to ensure that the plugin in installed before configuring."
    }
}
